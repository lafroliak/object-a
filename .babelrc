{
  "presets": ["next/babel"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@components": "./src/components",
          "@hooks": "./src/hooks",
          "@layouts": "./src/layouts",
          "@lib": "./src/lib",
          "@stores": "./src/stores",
          "@styles": "./src/styles",
          "@typings": "./src/typings"
        },
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    ]
  ]
}
